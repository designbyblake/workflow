{
    "name": "designbyblake-starter-npm",
    "version": "0.0.1",
    "author": "designbyblake (https://www.designbyblake.com)",
    "license": "MIT",
    "description": "Starter builds for front-end development",
    "repository": {
        "type": "git",
        "url": "https://github.com/designbyblake/workflow.git"
    },
    "bugs": {
        "url": "https://github.com/designbyblake/workflow/issues",
        "email": "designbyblake@gmail.com"
    },
    "devDependencies": {
        "ally": "^0.2.13",
        "autoprefixer": "^7.2.3",
        "babel-cli": "^6.26.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babelify": "^8.0.0",
        "browser-sync": "^2.18.13",
        "browserify": "^14.5.0",
        "casperjs": "^1.1.4",
        "clean-css": "^4.1.9",
        "clean-css-cli": "^4.1.10",
        "concurrently": "^3.5.1",
        "copyfiles": "^1.2.0",
        "del": "^3.0.0",
        "eslint": "^4.13.1",
        "eslint-nibble": "^4.2.1",
        "fs": "0.0.1-security",
        "grunt": "^1.0.1",
        "grunt-contrib-cssmin": "^2.2.1",
        "grunt-criticalcss": "^2.1.0",
        "grunt-twig-render": "^1.8.1",
        "gulp": "^3.9.1",
        "gulp-combine-mq": "^0.4.0",
        "gulp-html-prettify": "0.0.1",
        "gulp-postcss": "^7.0.0",
        "imagemin": "^5.3.1",
        "imagemin-mozjpeg": "^6.0.0",
        "imagemin-pngquant": "^5.0.1",
        "minifier": "^0.8.1",
        "node-sass": "^4.7.2",
        "onchange": "^3.3.0",
        "phantomcss": "^1.4.0",
        "phantomflow": "^1.2.2",
        "phantomjs-prebuilt": "^2.1.16",
        "picturefill": "^3.0.2",
        "postcss-cli": "^4.1.1",
        "resemblejs": "^2.5.0",
        "scrollmonitor": "^1.2.4",
        "slimerjs": "^0.10.3",
        "svgo": "^1.0.3",
        "uglify-js": "^2.4.11",
        "waypoints": "^4.0.1"
    },
    "scripts": {
        "scss": "node-sass --output-style expanded --indent-type tab --indent-width 1 -o build/_temp build/_sass",
        "combineMq": "gulp combineMq",
        "build:css": "npm run scss && npm run combineMq",
        "minify:css": "npm run build:css && cleancss -o style.css style.css",
        "watch:css": "onchange \"build/_sass/**/*.scss\" -- npm run build:css",
        "build:js": "browserify build/_js/_custom/scripts.js -d -o assets/js/scripts.js -t [ babelify --presets [ es2015 ] ] && npm run js:scripts:super",
        "build:js:prod": "browserify build/_js/_custom/scripts.js -o assets/js/scripts.js -t [ babelify --presets [ es2015 ] ] && minify -o assets/js/scripts.js assets/js/scripts.js && npm run js:scripts:super",
        "js:scripts": "cd build/_js/_lib && uglifyjs jquery.min.js  -o ../../assets/js/plugins_new.js",
        "js:scripts:super": "cd assets/js && cat plugins_new.js scripts.js > fishscripts.js ",
        "watch:js": "onchange \"build/_js/**/*.js\" -- npm run build:js",
        "eslint": "eslint \"build/_js/_custom/**\"",
        "nibble": "eslint-nibble \"build/_js/_custom/**\"",
        "images": "node build/scripts/image.js && npm run svg:min",
        "images:move": "node build/scripts/move-images.js",
        "svg:min": "svgo -f build/_images/ -o assets/images/",
        "prod": "npm run minify:css && npm run build:js:prod && grunt",
        "sync": "cd ../../.. && browser-sync start --proxy 'fishbowl.app' --files \"**/**/*.*\"",
        "twig": "grunt twigRender && gulp templates",
        "watch:twig": "onchange \"build/_twig/**/*.twig\" \"build/_json/**/*.json\" -- npm run twig",
        "watch:all": "concurrently 'npm run watch:css' 'npm run watch:js' 'npm run watch:twig'",
        "serve": "concurrently 'npm run sync' 'npm run watch:all'",
        "start": "npm run serve"
    }
}
